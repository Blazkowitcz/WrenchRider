{
  "useTabs": true,
  "tabWidth": 4,
  "semi": true,
  "singleQuote": true,
  "jsxSingleQuote": false,
  "trailingComma": "all",
  "bracketSpacing": true,
  "bracketSameLine": true,
  "arrowParens": "avoid",
  "importOrder": [
    "^[./]"
  ],
  "importOrderSortSpecifiers": true,
  "importOrderSeparation": true
}